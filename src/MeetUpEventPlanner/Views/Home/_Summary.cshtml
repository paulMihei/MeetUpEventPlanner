@model MeetUpEvent

<div class="col-sm-4">
    <section class="panel panel-default">
        <div class="panel-heading">@Model.Name</div>
        <div class="panel-body">
            Location: @Model.Location
            <div>
                <a class="btn btn-default" asp-action="Details" asp-route-id="@Model.Id">Details</a>
                <a class="btn btn-default" asp-action="Edit" asp-route-id="@Model.Id">Edit</a>
            </div>
        </div>
    </section>
</div>
